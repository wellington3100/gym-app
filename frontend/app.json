{
  "expo": {
    "name": "GymApp",
    "slug": "gym-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": null,
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6200ee"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Esta app necesita acceso a la cámara para escanear códigos QR de máquinas del gimnasio."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6200ee"
      },
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": null
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Permitir que GymApp acceda a la cámara para escanear códigos QR de máquinas del gimnasio."
        }
      ]
    ]
  }
}